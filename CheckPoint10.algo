 PROCEDURE permuter(var t : tab ;indice1 :integer ;indice2 :integer) ;
  Var
  aux :integer ;

  Begin
	Aux := t[indice1] ;
	t[indice1] :=t[indice2] ;
	t[indice2] :=aux ;
  end ;

  PROCEDURE tri_insertion(var t :tab ; n :integer)
  Var
     i ,k: integer ;

  Begin
   For i:= 2 to n do
    Begin
    k:=i;
       while (( k > 1 ) and ( t[k] < t[k-1] ) ) do
         begin
            permuter(t,k,k-1);
            k :=k-1;
         end;
    end ;
  end;